{
  "$schema": "https://json.schemastore.org/prettierrc",
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "printWidth": 100,
  "plugins": ["prettier-plugin-tailwindcss"]
}
